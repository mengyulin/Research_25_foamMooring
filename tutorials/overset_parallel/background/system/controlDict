FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            (overset fvMotionSolvers sixDoFMooring);

application     overInterDyMFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         10.8;

deltaT          0.0005;

writeControl    adjustableRunTime; // adjustable; timeStep;

writeInterval   0.225;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable no; //yes;

adjustTimeStep  yes;

maxCo           0.5; // 5.0
maxAlphaCo      0.5; // 2.0
maxDeltaT       0.01;

